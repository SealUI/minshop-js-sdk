page {
  background-color: #efeff4;
}

.shopping-cart {
  background-color: #fff;
}

.shopping-cart-empty {
  text-align: center;
  padding: 300rpx 0 100rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #c3c3c3;
}

.go-to-home {
  margin: 0 20rpx;
}

.product-list {
  border-top: 0.5px solid #e5e5e5;
  padding-bottom: 104rpx;
  background: #efeff4;
}

.product-item {
  position: relative;
  display: flex;
  align-items: center;
  padding: 40rpx 0;
  margin: 0 40rpx;
  height: 160rpx;
  border-bottom: 0.5px solid #e5e5e5;
}

.product-item:last-child {
  border-bottom: none;
}

.product-img {
  height: 160rpx;
  width: 160rpx;
  margin-right: 32rpx;
  border: 2rpx solid #ccc;
  box-sizing: border-box;
}

.product-detail {
  width: 422rpx;
}

.product-title {
  padding-right: 60rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #111;
  letter-spacing: 0;
  line-height: 42rpx;
  margin-bottom: 12rpx;
}

.product-spec {
  line-height: 37rpx;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #959595;
  letter-spacing: 0;
  margin-bottom: 21rpx;
}

.product-nomore-inventory {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #f8504f;
}

.product-delete {
  position: absolute;
  right: 0;
  top: 30rpx;
  width: 50rpx;
  height: 50rpx;
  text-align: right;
  font-size: 30rpx;
  color: #87858f;
}

.product-select-tool {
  display: flex;
  justify-content: space-between;
}

.product-price {
  display: inline-block;
  font-weight: 700;
  line-height: 60rpx;
  font-family: HelveticaNeue;
  font-size: 26rpx;
  color: #111;
  letter-spacing: 0;
}

.product-select-box {
  font-size: 16px;
  background-color: #fff;
  border: 2rpx solid #ccc;
}

.product-select-box-item {
  display: inline-block;
  line-height: 60rpx;
  height: 60rpx;
  width: 60rpx;
  font-size: 22rpx;
  text-align: center;
  color: #87858f;
}

.product-quantity {
  min-width: 74rpx;
  font-size: 24rpx;
  color: #111;
  border-left: 2rpx #ccc solid;
  border-right: 2rpx #ccc solid;
}

.shopping-cart-foot {
  position: fixed;
  border-top: 0.5px solid #e5e5e5;
  height: 98rpx;
  font-size: 32rpx;
  line-height: 98rpx;
  bottom: 0;
  left: 0;
  width: 100%;
  color: #3c3b4b;
  background-color: #fff;
  box-sizing: border-box;
}

.shopping-cart-select-all {
  height: 98rpx;
  display: flex;
  align-items: center;
  float: left;
  margin-left: 40rpx;
  color: #3c3b4b;
  margin-right: 48rpx;
}

.shopping-cart-select-all::before {
  font-size: 40rpx;
  margin-right: 20rpx;
}

.total-price {
  float: left;
  margin-right: 30rpx;
  opacity: 0.92;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #e23737;
  line-height: 98rpx;
}

.total-price-number {
  color: #fd544a;
}

.shopping-cart-payment {
  float: right;
  height: 98rpx;
  line-height: 98rpx;
  width: 200rpx;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  background-color: #fd544a;
  border-radius: 0;
  box-sizing: border-box;
}

.shopping-cart-payment::after {
  border: 0;
}

.shopping-cart-payment--active::after {
  background: rgba(0, 0, 0, 0.1);
}

.error-message {
  text-align: center;
  font-size: 28rpx;
  line-height: 40rpx;
}

.product-group {
  margin-bottom: 16rpx;
  background: #fff;
}

.red {
  color: #e23737;
}

.enough-discount-tip {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #e23737;
  letter-spacing: 0;
  background: pink;
  padding: 0 40rpx;
  line-height: 64rpx;
  height: 64rpx;
}

.more-discount-tip {
  display: flex;
  margin: 0 40rpx;
  border-bottom: 1rpx solid #ccc;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #111;
  letter-spacing: 0;
  height: 98rpx;
  align-items: center;
}

.buy-more {
  width: 105rpx;
}

.more-tip-text {
  flex: 1;
}

.group-checkbox {
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  border: 2rpx solid #959595;
  box-sizing: border-box;
  margin:0 24rpx 0 0;
}

.ifanrin-check-icon {
  color: #e23737;
  position: relative;
  border: none;
}

.ifanrin-check-icon::before {
  position: absolute !important;
  font-size: 32rpx;
  top: 0;
  left: 0;
}

.preferential-modal {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.45);
  display: flex;
  flex-direction: column-reverse;
}

.preferential-modal-content {
  width: 750rpx;
  height: 822rpx;
  background: #fff;
  position: relative;
  padding: 48rpx 48rpx 0;
  box-sizing:border-box;
  -webkit-overflow-scrolling:touch;
}

.close-modal-btn {
  width: 28rpx;
  height: 28rpx;
  color: #000;
  font-size: 50rpx;
  z-index: 4;
}

.modal-title {
  font-family: PingFangSC-Regular;
  font-size: 64rpx;
  color: #111;
  line-height: 186rpx;
  height: 186rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0 48rpx;
  box-sizing: border-box;
  background: #fff;
  z-index: 3;
  display: flex;
  justify-content: space-between;
}

.present-text {
  padding: 48rpx 0;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #707070;
  line-height: 48rpx;
  border-bottom: 1rpx solid #ccc;
}

.present-text:last-child {
  border: none;
}

.presale {
  padding: 48rpx 48rpx 16rpx;
}

.presale-title {
  font-family: PingFangSC-Regular;
  font-size: 38rpx;
  color: #111;
  letter-spacing: 0;
  line-height: 53rpx;
  margin-bottom: 46rpx;
}

.presale-item {
  margin-bottom: 48rpx;
  padding-left: 28rpx;
  box-sizing: border-box;
}

.presale-item-name::before {
  display: block;
  content: '';
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background: #e23737;
  position: absolute;
  left: -28rpx;
  top: 50%;
  transform: translateY(-50%);
}

.presale-item-name {
  position: relative;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  line-height: 42rpx;
  color: #111;
  letter-spacing: 0;
  display: inline-block;
}

.presale-item-text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #000;
  letter-spacing: 0;
  line-height: 42rpx;
}

.special-item {
  height: 110rpx;
  line-height: 110rpx;
  display: flex;
  align-items: center;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #111;
  letter-spacing: 0;
  border-bottom: 1rpx solid #ccc;
}

.special-item .arrow-icon {
  margin-left: 48rpx;
}

.special-text {
  width: 600rpx;
}

.coupon-item {
  position: relative;
  margin-bottom: 48rpx;
  height: 240rpx;
  overflow: hidden;
  color: #111;
  box-sizing: border-box;
}

.coupon-over-time {
  position: absolute;
  right: 16rpx;
  bottom: 16rpx;
  box-sizing: border-box;
  width: 102rpx;
  height: 102rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 2;
}

.coupon-over-time-circle {
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
  border: 1rpx solid #fff;
  font-family: Helvetica-Bold;
  font-size: 24rpx;
  line-height: 85rpx;
  text-align: center;
  color: #ccc;
  transform: rotate(-45deg);
}

.coupon-item__checkbox {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 44rpx;
  line-height: 44rpx;
  color: #b8b8b8;
}

.coupon-item__checkbox--selected {
  color: #e23737;
}

.coupon-item--left, .coupon-item--right {
  display: flex;
  flex-flow: column;
}

.coupon-item-content {
  width: 100%;
  display: flex;
  height: 165rpx;
  background: #cabd73;
}

.coupon-item-bottom {
  width: 100%;
  height: 80rpx;
  background-size: 100% 80rpx;
  background-repeat: no-repeat;
  background-position: top center;
  margin-top: -5rpx;
}

.coupon-item--left {
  width: 212rpx;
  font-size: 34rpx;
  background-position: left top;
  box-sizing: border-box;
  position: relative;
}

.coupon__name {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #fff;
  line-height: 42rpx;
  margin: 40rpx 0 8rpx;
}

.coupon__description {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #fff;
  line-height: 80rpx;
  padding-left: 32rpx;
  box-sizing: border-box;
}

.coupon__valid-time {
  font-family: Helvetica-Light;
  font-size: 20rpx;
  color: #fff;
  line-height: 28rpx;
}

.coupon__face-value {
  margin: 40rpx 0 16rpx 59rpx;
  font-family: Helvetica-Bold;
  font-size: 64rpx;
  line-height: 77rpx;
  color: #fff;
  position: relative;
}

.coupon__face-value::before {
  content: '\00A5';
  display: block;
  position: absolute;
  top: -10rpx;
  left: -27rpx;
  font-size: 40rpx;
  color: #fff;
}

.coupon-item--center {
  flex: 1;
  font-size: 24rpx;
  line-height: 32rpx;
  color: #b8b8b8;
  background-position: right top;
}

.coupon__minimum_amount {
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #959595;
}

.coupon-item--right {
  width: 132rpx;
  height: 100%;
  margin-right: 40rpx;
}

.go-use {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #cabd73;
  width: 132rpx;
  height: 40rpx;
  background: #fff;
  text-align: center;
  line-height: 40rpx;
  margin-top: 56rpx;
}

.cant-use-cover {
  position: absolute;
  left: 0;
  top: 0;
  width: 654rpx;
  height: 240rpx;
  background: rgba(17, 17, 17, 0.30);
}

.price-box {
  display: flex;
  height: 57rpx;
  align-items: center;
  line-height: 57rpx;
  margin-top: 50rpx;
  margin-bottom: 16rpx;
}

.standard-title {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #959595;
  line-height: 37rpx;
}

.toast {
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  top: 701rpx;
  width: 320rpx;
  height: 98rpx;
  background: #111;
  opacity: 0.5;
  border-radius: 4rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 5;
}

.toast__text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
}

.modal-padding {
  padding-top: 138rpx;
}

.group-btn {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.select-all {
  margin-left: 0;
}

.coupon-container {
  overflow-y: scroll;
  height: 750rpx;
}